{
    "$schema": "https://raw.githubusercontent.com/LGUG2Z/komorebi/v0.1.36/schema.json",
    "app_specific_configuration_path": "$Env:KOMOREBI_CONFIG_HOME/applications.json",
    "resize_delta": 50,
    "window_container_behaviour": "Create",
    "cross_monitor_move_behaviour": "Insert",
    "unmanaged_window_operation_behaviour": "Op",
    "mouse_follows_focus": false,
    "border_width": 20,
    "border": false,
    "border_colours": {
        "single": {
            "r": 66,
            "g": 165,
            "b": 245
        },
        "stack": {
            "r": 0,
            "g": 165,
            "b": 66
        },
        "monocle": {
            "r": 255,
            "g": 51,
            "b": 153
        }
    },
    "default_workspace_padding": 10,
    "default_container_padding": 10,
    "monitors": [
        {
            "workspaces": [
                {
                    "name": "I",
                    "layout": "BSP",
                    "workspace_rules": [
                        {
                            "kind": "Exe",
                            "id": "EpicGamesLauncher.exe"
                        },
                        {
                            "kind": "Exe",
                            "id": "Battle.net.exe"
                        },
                        {
                            "kind": "Exe",
                            "id": "steamwebhelper.exe"
                        },
                        {
                            "kind": "Exe",
                            "id": "javaw.exe"
                        }
                    ],
                    "layout_rules": {}
                },
                {
                    "name": "II",
                    "layout": "BSP",
                    "initial_workspace_rules": [
                        {
                            "kind": "Exe",
                            "id": "brave.exe"
                        },
                        {
                            "kind": "Exe",
                            "id": "zen.exe"
                        }
                    ],
                    "layout_rules": {}
                },
                {
                    "name": "III",
                    "layout": "BSP",
                    "workspace_rules": [
                        {
                            "kind": "Exe",
                            "id": "WindowsTerminal.exe"
                        },
                        {
                            "kind": "Exe",
                            "id": "alacritty.exe"
                        },
                        {
                            "kind": "Exe",
                            "id": "wezterm-gui.exe"
                        }
                    ],
                    "layout_rules": {}
                },
                {
                    "name": "IV",
                    "layout": "BSP",
                    "initial_workspace_rules": [
                        {
                            "kind": "Exe",
                            "id": "Obsidian.exe"
                        },
                        {
                            "kind": "Exe",
                            "id": "msedge.exe"
                        }
                    ],
                    "layout_rules": {}
                },
                {
                    "name": "V",
                    "layout": "BSP",
                    "workspace_rules": [
                        {
                            "kind": "Exe",
                            "id": "Discord.exe"
                        }

                    ]
                },
                {
                    "name": "VI",
                    "initial_workspace_rules": [
                        {
                            "kind": "Exe",
                            "id": "explorer.exe"
                        },
                        {
                            "kind": "Exe",
                            "id": "BakkesMod.exe"
                        }
                    ]
                }
            ]
        }
    ],
    "alt_focus_hack": true,
    "window_hiding_behaviour": "Cloak",
    "monitor_index_preferences": {}
}
